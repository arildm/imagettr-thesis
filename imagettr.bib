@article{SimonDobnikModularmechanisticnetworks2017,
  title = {Modular {{Mechanistic Networks}}: {{On Bridging Mechanistic}} and {{Phenomenological Models}} with {{Deep Neural Networks}} in {{Natural Language Processing}}},
